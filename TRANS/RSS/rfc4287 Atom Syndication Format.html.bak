<br><br><br>&nbsp;&nbsp;&nbsp; atomUpdated
          &amp; <br>&nbsp;&nbsp;&nbsp; extensionElement*)
      }

   <br><br># atom:content

   <br>atomInlineTextContent =
      element atom:content {
         <br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
         <br>&nbsp;&nbsp;&nbsp; attribute type { "text" | "html" }?,
         (text)*
      }

   <br>&nbsp;&nbsp;&nbsp; atomInlineXHTMLContent =
елемент atom:content {
         <br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; atomCommonAttributes,
<br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; атрибут type { "xhtml" },
         xhtmlDiv
      }

   <br>&nbsp;&nbsp;&nbsp; atomInlineOtherContent =
елемент atom:content {
         <br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; atomCommonAttributes,
<br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; атирибут type { atomMediaType }?,
         <br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; (text|anyElement)*
      }
<br>&nbsp;&nbsp;&nbsp; atomOutOfLineContent = елемент atom:content {
<br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; atomCommonAttributes,
         <br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; атрибут type { atomMediaType }?,
         <br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; attribute src { atomUri },
<br>&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp; empty
      }

   <br>&nbsp;&nbsp;&nbsp; atomContent = atomInlineTextContent
    | atomInlineXHTMLContent
    | atomInlineOtherContent
    | atomOutOfLineContent

   <br><br># atom:author

   <br>atomAuthor = елемент atom:author { atomPersonConstruct }
<br><br># atom:category
<br>
   atomCategory =
елемент atom:category {
<br><br><br><span class="grey" style="color: rgb(119, 119, 119);">Nottingham &amp; Sayre          Standards Track                    [Page 38]<br><br></span>
        <hr class="noprint" style="color: rgb(0, 0, 0); font-family: &quot;Times New Roman&quot;; font-size: 13.3333px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: normal; letter-spacing: normal; orphans: 2; text-align: start; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-style: initial; text-decoration-color: initial; width: 96ex;"
align="left">
        <br><a name="page-39" id="page-39" href="#page-39" class="invisible" style="text-decoration: none; color: white;"> </a>
<span class="grey" style="color: rgb(119, 119, 119);"><a href="" style="color: rgb(119, 119, 119);">RFC 4287</a>                      Atom Format                  December 2005</span>

<br><br><br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
<br>&nbsp;&nbsp;&nbsp; атрибут term { text },
         <br>&nbsp;&nbsp;&nbsp; атрибут scheme { atomUri }?,
         <br>&nbsp;&nbsp;&nbsp; атрибут label { text }?,
<br>&nbsp;&nbsp;&nbsp; undefinedContent
      }

   <br><br># atom:contributor

   <br>atomContributor = елемент atom:contributor { atomPersonConstruct }

   <br><br># atom:generator

   <br>atomGenerator = елемент atom:generator {
      <br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
      <br>&nbsp;&nbsp;&nbsp; атрибут uri { atomUri }?,
<br>&nbsp;&nbsp;&nbsp; атрибут version { text }?,
      text
   }

   <br><br># atom:icon

   <br>atomIcon = елемент atom:icon {
      <br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
      <br>&nbsp;&nbsp;&nbsp; (atomUri)
   }

   <br><br># atom:id

   <br>atomId = елемент atom:id {
<br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
      <br>&nbsp;&nbsp;&nbsp; (atomUri)
   }

   <br><br># atom:logo

   <br>atomLogo = елемент atom:logo {
<br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
      <br>&nbsp;&nbsp;&nbsp; (atomUri)
   }

   <br><br># atom:link

   <br>atomLink =
елемент atom:link {
         <br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
         <br>&nbsp;&nbsp;&nbsp; атрибут href { atomUri },
<br>&nbsp;&nbsp;&nbsp; атрибут rel { atomNCName | atomUri }?,



<span class="grey" style="color: rgb(119, 119, 119);"><br><br><br>Nottingham &amp; Sayre          Standards Track                    [Page 39]<br><br></span>
        <hr class="noprint" style="color: rgb(0, 0, 0); font-family: &quot;Times New Roman&quot;; font-size: 13.3333px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: normal; letter-spacing: normal; orphans: 2; text-align: start; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-style: initial; text-decoration-color: initial; width: 96ex;"
align="left">
        <br><a name="page-40" id="page-40" href="#page-40" class="invisible" style="text-decoration: none; color: white;"> </a>
<span class="grey" style="color: rgb(119, 119, 119);"><a href="" style="color: rgb(119, 119, 119);">RFC 4287</a>                      Atom Format                  December 2005</span>

<br><br><br>&nbsp;&nbsp;&nbsp; атрибут type { atomMediaType }?,
<br>&nbsp;&nbsp;&nbsp; атрибут hreflang { atomLanguageTag }?,
<br>&nbsp;&nbsp;&nbsp; атрибут title { text }?,
<br>&nbsp;&nbsp;&nbsp; атрибут length { text }?,
<br>&nbsp;&nbsp;&nbsp; undefinedContent
      }

   <br><br># atom:published

   <br>atomPublished = елемент atom:published { atomDateConstruct }

   <br><br># atom:rights

   <br>atomRights = елемент atom:rights { atomTextConstruct }

   <br><br># atom:source

   <br>atomSource =
елемент atom:source {
         <br>&nbsp;&nbsp;&nbsp; atomCommonAttributes,
         <br>&nbsp;&nbsp;&nbsp; (atomAuthor*
          &amp; <br>&nbsp;&nbsp;&nbsp; atomCategory*
          &amp; <br>&nbsp;&nbsp;&nbsp; atomContributor*
          &amp; <br>&nbsp;&nbsp;&nbsp; atomGenerator?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomIcon?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomId?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomLink*
          &amp; <br>&nbsp;&nbsp;&nbsp; atomLogo?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomRights?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomSubtitle?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomTitle?
          &amp; <br>&nbsp;&nbsp;&nbsp; atomUpdated?
          &amp; <br>&nbsp;&nbsp;&nbsp; extensionElement*)
      }

   <br><br># atom:subtitle

   <br>atomSubtitle = елемент atom:subtitle { atomTextConstruct }

   <br><br># atom:summary
<br>
   atomSummary = елемент atom:summary { atomTextConstruct }

   <br><br># atom:title

   <br>atomTitle = елемент atom:title { atomTextConstruct }

   <br><br># atom:updated
<br>atomUpdated = елемент atom:updated { atomDateConstruct }<br><br><br><span class="grey"
style="color: rgb(119, 119, 119);">Nottingham &amp; Sayre          Standards Track                    [Page 40]<br><br></span>
        <hr class="noprint" style="color: rgb(0, 0, 0); font-family: &quot;Times New Roman&quot;; font-size: 13.3333px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: normal; letter-spacing: normal; orphans: 2; text-align: start; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-style: initial; text-decoration-color: initial; width: 96ex;"
align="left">
        <br><a name="page-41" id="page-41" href="#page-41" class="invisible" style="text-decoration: none; color: white;"> </a>
<span class="grey" style="color: rgb(119, 119, 119);"><a href="" style="color: rgb(119, 119, 119);">RFC 4287</a>                      Atom Format                  December 2005</span>

<br><br><br># Низькорівневі прості типи<br>atomNCName = xsd:string { minLength = "1" pattern = "[^:]*" }

   <br><br># Медія тип завжди містить щонайменьше одну косу лінію<br>atomMediaType = xsd:string { pattern = ".+/.+" }

   <br><br># Як визначено в <a href="https://tools.ietf.org/html/rfc3066">RFC 3066&nbsp;</a>
<br>   atomLanguageTag = xsd:string {
      pattern = "[A-Za-z]{1,8}(-[A-Za-z0-9]{1,8})*"
   }
<br><br># Не обмежено; не досить ясно, як IRI пасує до xsd:anyURI, так що не намагаймось обмежувати це тут<br>   atomUri = text

   <br><br># Електронна пошта завжди містить щонайменьше один символ @
   <br>atomEmailAddress = xsd:string { pattern = ".+@.+" }

   <br><br># Просте розширення<br>simpleExtensionElement =
елемент * - atom:* {
         text
      }

   <br><br># Структуроване розширення<br>
   structuredExtensionElement =
елемент * - atom:* {
         <br>&nbsp;&nbsp;&nbsp; (атрибут * { text }+, (text|anyElement)*)
       | <br>&nbsp;&nbsp;&nbsp; (атрибут * { text }*,
          (text?, anyElement+, (text|anyElement)*))
      }
<br><br># Інша розширюваність<br>extensionElement =
      simpleExtensionElement | structuredExtensionElement

   undefinedAttribute =
     attribute * - (xml:base | xml:lang | local:*) { text }
<br>&nbsp;&nbsp;&nbsp; undefinedContent = (text|anyForeignElement)*




<span class="grey" style="color: rgb(119, 119, 119);"><br><br><br>Nottingham &amp; Sayre          Standards Track                    [Page 41]<br><br></span>
        <hr class="noprint" style="color: rgb(0, 0, 0); font-family: &quot;Times New Roman&quot;; font-size: 13.3333px; font-style: normal; font-variant-ligatures: normal; font-variant-caps: normal; font-weight: normal; letter-spacing: normal; orphans: 2; text-align: start; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-stroke-width: 0px; text-decoration-style: initial; text-decoration-color: initial; width: 96ex;"
align="left">
        <br><a name="page-42" id="page-42" href="#page-42" class="invisible" style="text-decoration: none; color: white;"> </a>
<span class="grey" style="color: rgb(119, 119, 119);"><a href="" style="color: rgb(119, 119, 119);">RFC 4287</a>                      Atom Format                  December 2005</span>

<br><br><br>&nbsp;&nbsp;&nbsp; anyElement =
елемент * {
         (attribute * { text }
          | text
          | anyElement)*
      }

   <br>&nbsp;&nbsp;&nbsp; anyForeignElement =
      елемент * - atom:* {
         (атрибут * { text }
          | text
          | anyElement)*
      }

   <br><br># XHTML
<br>
   anyXHTML = елемент xhtml:* {
      (елемент * { text }
       | text
       | anyXHTML)*
   }
<br>xhtmlDiv = елемент xhtml:div {
      (атрибут * { text }
       | text
       | anyXHTML)*
   }
