            .map(str -&gt; dynamicTest(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">test</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span> + str, () -&gt; { <span
class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">/* ... */</span> }));
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@TestFactory</span>
    Stream&lt;DynamicTest&gt; dynamicTestsFromIntStream() {
        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// Генерує тести для перших 10 парних цілих.</span>
        <span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">return</span> IntStream.iterate(<span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">0</span>, n -&gt; n + <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">2</span>).limit(<span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">10</span>)
            .mapToObj(n -&gt; dynamicTest(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">test</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span> + n, () -&gt; assertTrue(n % <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">2</span> == <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">0</span>)));
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@TestFactory</span>
    Stream&lt;DynamicTest&gt; generateRandomNumberOfTests() {

        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// Генерує випадкові додатні цілі між 0 та 100, доки не натрапить на число,</span>
        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// що ділиться на 7 нарівно.</span>
        <span class="predefined-type" style="box-sizing: border-box; color: inherit;">Iterator</span>&lt;<span
class="predefined-type" style="box-sizing: border-box; color: inherit;">Integer</span>&gt; inputGenerator = <span
class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">new</span> <span
class="predefined-type" style="box-sizing: border-box; color: inherit;">Iterator</span>&lt;&gt;() {

            <span class="predefined-type" style="box-sizing: border-box; color: inherit;">Random</span> random = <span
class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">new</span> <span
class="predefined-type" style="box-sizing: border-box; color: inherit;">Random</span>();
            <span class="type" style="box-sizing: border-box; font-weight: bold;">int</span> current;

            <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@Override</span>
            <span class="directive" style="box-sizing: border-box; font-weight: bold;">public</span> <span
class="type" style="box-sizing: border-box; font-weight: bold;">boolean</span> hasNext() {
                current = random.nextInt(<span class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">100</span>);
                <span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">return</span> current % <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">7</span> != <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">0</span>;
            }

            <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@Override</span>
            <span class="directive" style="box-sizing: border-box; font-weight: bold;">public</span> <span
class="predefined-type" style="box-sizing: border-box; color: inherit;">Integer</span> next() {
                <span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">return</span> current;
            }
        };

        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// Генерує дисплейні імена, як input:5, input:37, input:85, etc.</span>
        Function&lt;<span class="predefined-type" style="box-sizing: border-box; color: inherit;">Integer</span>, <span
class="predefined-type" style="box-sizing: border-box; color: inherit;">String</span>&gt; displayNameGenerator = (input) -&gt; <span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">input:</span><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span> + input;

        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// Виконує тести, базуючись на вхідному значенні.</span>
        ThrowingConsumer&lt;<span class="predefined-type" style="box-sizing: border-box; color: inherit;">Integer</span>&gt; testExecutor = (input) -&gt; assertTrue(input % <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">7</span> != <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">0</span>);

        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// Повертає потік з динамічних тестів.</span>
        <span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">return</span> DynamicTest.stream(inputGenerator, displayNameGenerator, testExecutor);
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@TestFactory</span>
    Stream&lt;DynamicNode&gt; dynamicTestsWithContainers() {
        <span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">return</span> Stream.of(<span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">A</span><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, <span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">B</span><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, <span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">C</span><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>)
            .map(input -&gt; dynamicContainer(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">Container </span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span> + input, Stream.of(
                dynamicTest(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">not null</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, () -&gt; assertNotNull(input)),
                dynamicContainer(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">properties</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, Stream.of(
                    dynamicTest(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">length &gt; 0</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, () -&gt; assertTrue(input.length() &gt; <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">0</span>)),
                    dynamicTest(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">not empty</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, () -&gt; assertFalse(input.isEmpty()))
                ))
            )));
    }
}</code></pre>
              </div>
