<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.platform.launcher.TestPlan</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.platform.launcher.core.LauncherConfig</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
