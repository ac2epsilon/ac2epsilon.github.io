<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">java.util.logging.Logger</span>;

<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.jupiter.api.BeforeEach</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.jupiter.api.DisplayName</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.jupiter.api.RepeatedTest</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.jupiter.api.RepetitionInfo</span>;
<span class="keyword" style="box-sizing: border-box; color: rgb(0, 0, 0); font-weight: bold;">import</span> <span
class="include" style="box-sizing: border-box; color: rgb(85, 85, 85);">org.junit.jupiter.api.TestInfo</span>;

<span class="type" style="box-sizing: border-box; font-weight: bold;">class</span> <span
class="class" style="box-sizing: border-box; color: rgb(68, 85, 136); font-weight: bold;">RepeatedTestsDemo</span> {

    <span class="directive" style="box-sizing: border-box; font-weight: bold;">private</span> <span
class="predefined-type" style="box-sizing: border-box; color: inherit;">Logger</span> logger = <span
class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// ...</span>

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@BeforeEach</span>
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> beforeEach(TestInfo testInfo, RepetitionInfo repetitionInfo) {
        <span class="type" style="box-sizing: border-box; font-weight: bold;">int</span> currentRepetition = repetitionInfo.getCurrentRepetition();
        <span class="type" style="box-sizing: border-box; font-weight: bold;">int</span> totalRepetitions = repetitionInfo.getTotalRepetitions();
        <span class="predefined-type" style="box-sizing: border-box; color: inherit;">String</span> methodName = testInfo.getTestMethod().get().getName();
        logger.info(<span class="predefined-type" style="box-sizing: border-box; color: inherit;">String</span>.format(<span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">About to execute repetition %d of %d for %s</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>, <span
class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">//</span>
            currentRepetition, totalRepetitions, methodName));
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@RepeatedTest</span>(<span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">10</span>)
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> repeatedTest() {
        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// ...</span>
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@RepeatedTest</span>(<span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">5</span>)
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> repeatedTestWithRepetitionInfo(RepetitionInfo repetitionInfo) {
        assertEquals(<span class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">5</span>, repetitionInfo.getTotalRepetitions());
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@RepeatedTest</span>(value = <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">1</span>, name = <span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">{displayName} {currentRepetition}/{totalRepetitions}</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>)
    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@DisplayName</span>(<span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">Repeat!</span><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>)
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> customDisplayName(TestInfo testInfo) {
        assertEquals(testInfo.getDisplayName(), <span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">Repeat! 1/1</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>);
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@RepeatedTest</span>(value = <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">1</span>, name = RepeatedTest.LONG_DISPLAY_NAME)
    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@DisplayName</span>(<span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">Details...</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>)
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> customDisplayNameWithLongPattern(TestInfo testInfo) {
        assertEquals(testInfo.getDisplayName(), <span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
class="content" style="box-sizing: border-box; color: rgb(221, 17, 68);">Details... :: repetition 1 of 1</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>);
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@RepeatedTest</span>(value = <span
class="integer" style="box-sizing: border-box; color: rgb(0, 153, 153);">5</span>, name = <span
class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span class="delimiter"
style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span class="content"
style="box-sizing: border-box; color: rgb(221, 17, 68);">Wiederholung {currentRepetition} von {totalRepetitions}</span><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span></span>)
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> repeatedTestInGerman() {
        <span class="comment" style="box-sizing: border-box; color: rgb(153, 153, 136); font-style: italic;">// ...</span>
    }

}</code></pre>
              </div>
            </div>
            <div class="paragraph" style="box-sizing: border-box; margin: 0px; padding: 0px; direction: ltr;">
              <p style="box-sizing: border-box; margin: 0px 0px 1.25rem; padding: 0px; direction: ltr; font-family: inherit; font-weight: 400; font-size: 1.0625rem; line-height: 1.6; text-rendering: optimizelegibility; letter-spacing: -0.01em;">Коли
                використовувати <code style="box-sizing: border-box; font-family: &quot;Droid Sans Mono&quot;, &quot;DejaVu Sans Mono&quot;, monospace; font-size: 0.9375em; font-weight: 400; color: rgba(0, 0, 0, 0.9); letter-spacing: 0px; padding: 0.1em 0.5ex; word-spacing: -0.15em; background-color: rgb(247, 247, 248); border-radius: 4px; line-height: 1.45; text-rendering: optimizespeed; overflow-wrap: break-word; font-style: normal !important;">ConsoleLauncher</code><span>
                  з вімкненою темою юнікоду, виконання </span><span></span><code
                  style="box-sizing: border-box; font-family: &quot;Droid Sans Mono&quot;, &quot;DejaVu Sans Mono&quot;, monospace; font-size: 0.9375em; font-weight: 400; color: rgba(0, 0, 0, 0.9); letter-spacing: 0px; padding: 0.1em 0.5ex; word-spacing: -0.15em; background-color: rgb(247, 247, 248); border-radius: 4px; line-height: 1.45; text-rendering: optimizespeed; overflow-wrap: break-word; font-style: normal !important;">RepeatedTestsDemo</code><span>
                  призведе до наступного виводу на консоль</span>.</p>
            </div>
            <div class="literalblock" style="box-sizing: border-box; margin: 0px 0px 1.25em; padding: 0px; direction: ltr;">
              <div class="content" style="box-sizing: border-box; margin: 0px; padding: 0px; direction: ltr;">
                <pre style="box-sizing: border-box; font-family: &quot;Droid Sans Mono&quot;, &quot;DejaVu Sans Mono&quot;, monospace; font-size: 1em; white-space: pre-wrap; margin: 0px; padding: 1em; direction: ltr; line-height: 1.45; color: rgba(0, 0, 0, 0.9); font-weight: 400; text-rendering: optimizespeed; background: rgb(247, 247, 248); border-radius: 4px; overflow-wrap: break-word;">├─ RepeatedTestsDemo ✔
│  ├─ repeatedTest() ✔
│  │  ├─ repetition 1 of 10 ✔
