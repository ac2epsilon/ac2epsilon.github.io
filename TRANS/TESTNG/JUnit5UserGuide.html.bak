<span class="type" style="box-sizing: border-box; font-weight: bold;">class</span> <span
class="class" style="box-sizing: border-box; color: rgb(68, 85, 136); font-weight: bold;">StandardTests</span> {

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@BeforeAll</span>
    <span class="directive" style="box-sizing: border-box; font-weight: bold;">static</span> <span
class="type" style="box-sizing: border-box; font-weight: bold;">void</span> initAll() {
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@BeforeEach</span>
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> init() {
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@Test</span>
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> succeedingTest() {
    }

    <span class="annotation" style="box-sizing: border-box; color: rgb(0, 0, 119);">@Test</span>
    <span class="type" style="box-sizing: border-box; font-weight: bold;">void</span> failingTest() {
        fail(<span class="string" style="box-sizing: border-box; color: rgb(221, 34, 0);"><span
class="delimiter" style="box-sizing: border-box; color: rgb(221, 17, 68);">"</span><span
