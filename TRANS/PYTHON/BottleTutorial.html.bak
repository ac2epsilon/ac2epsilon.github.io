          <dt id="bottle.HTTPError" style="font-weight: bold;"><em class="property">exception<span>&nbsp;</span></em><code
              class="descname" style="font-size: 1.2em; padding: 0px 0.2em; background-color: transparent; font-weight: bold;">HTTPError</code><span
              class="sig-paren" style="font-size: larger;">(</span><em>status=None</em>,<span>&nbsp;</span><em>body=None</em>,<span>&nbsp;</span><em>exception=None</em>,<span>&nbsp;</span><em>traceback=None</em>,<span>&nbsp;</span><em>**more_headers</em><span
              class="sig-paren" style="font-size: larger;">)</span><a class="reference internal"
