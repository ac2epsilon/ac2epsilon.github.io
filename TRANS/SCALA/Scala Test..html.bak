 style="color: rgb(0, 0, 139);">with</span> <span  class="stType"  style="color: rgb(65, 141, 167);">ShouldMatchers</span> <span
 class="stReserved"  style="color: rgb(0, 0, 139);">with</span> <span  class="stType"
 style="color: rgb(65, 141, 167);">WebBrowser</span> {

  <span  class="stReserved"  style="color: rgb(0, 0, 139);">implicit</span> <span
 class="stReserved"  style="color: rgb(0, 0, 139);">val</span> webDriver: <span
 class="stType"  style="color: rgb(65, 141, 167);">WebDriver</span> = <span  class="stReserved"
 style="color: rgb(0, 0, 139);">new</span> <span  class="stType"  style="color: rgb(65, 141, 167);">HtmlUnitDriver</span>

