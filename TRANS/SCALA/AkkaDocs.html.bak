                          &#1089;&#1086;&#1090;&#1077;&#1085; &#1084;&#1072;&#1096;&#1080;&#1085;. &#1050;&#1083;&#1102;&#1095; &#1076;&#1083;&#1103; &#1074;&#1087;&#1088;&#1086;&#1074;&#1072;&#1076;&#1078;&#1077;&#1085;&#1085;&#1103; &#1094;&#1100;&#1086;&#1075;&#1086; &#1094;&#1077; &#1087;&#1077;&#1088;&#1077;&#1093;&#1110;&#1076;
                          &#1074;&#1110;&#1076; &#1074;&#1110;&#1076;&#1076;&#1072;&#1083;&#1077;&#1085;&#1086;&#1075;&#1086; &#1076;&#1086; &#1083;&#1086;&#1082;&#1072;&#1083;&#1100;&#1085;&#1086;&#1075;&#1086; &#1096;&#1083;&#1103;&#1093;&#1086;&#1084; &#1086;&#1087;&#1090;&#1080;&#1084;&#1110;&#1079;&#1072;&#1094;&#1110;&#1111;,
                          &#1079;&#1072;&#1084;&#1110;&#1090;&#1100; &#1090;&#1086;&#1075;&#1086;, &#1097;&#1086;&#1073; &#1085;&#1072;&#1084;&#1072;&#1075;&#1072;&#1090;&#1080;&#1089;&#1100; &#1087;&#1088;&#1086;&#1081;&#1090;&#1080; &#1074;&#1110;&#1076; &#1083;&#1086;&#1082;&#1072;&#1083;&#1100;&#1085;&#1086;&#1075;&#1086; &#1076;&#1086;
                          &#1074;&#1110;&#1076;&#1076;&#1072;&#1083;&#1077;&#1085;&#1086;&#1075;&#1086;, &#1096;&#1083;&#1103;&#1075;&#1086;&#1084; &#1091;&#1079;&#1072;&#1075;&#1072;&#1083;&#1100;&#1085;&#1077;&#1085;&#1085;&#1103;. &#1044;&#1080;&#1074;&#1110;&#1090;&#1100;&#1089;&#1103;&nbsp;<span
class="Apple-converted-space"></span><a class="reference external" href="http://doc.akka.io/docs/misc/smli_tr-94-29.pdf"
style="color: rgb(21, 169, 206); text-decoration: none;">&#1094;&#1077;&#1081;
