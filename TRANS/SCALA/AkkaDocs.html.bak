                          &#1108; &#1074;&#1110;&#1088;&#1085;&#1080;&#1084; &#1076;&#1083;&#1103; Akka, &#1085;&#1077; &#1086;&#1073;&#1086;&#1074;'&#1103;&#1079;&#1082;&#1086;&#1074;&#1086; &#1074;&#1110;&#1088;&#1085;&#1077; &#1076;&#1083;&#1103;
                          &#1079;&#1072;&#1089;&#1090;&#1086;&#1089;&#1091;&#1074;&#1072;&#1085;&#1085;&#1103;, &#1097;&#1086; &#1074;&#1080;&#1082;&#1086;&#1088;&#1080;&#1089;&#1090;&#1086;&#1074;&#1091;&#1108; &#1081;&#1086;&#1075;&#1086;, &#1086;&#1089;&#1082;&#1110;&#1083;&#1100;&#1082;&#1080; &#1088;&#1086;&#1079;&#1088;&#1086;&#1073;&#1082;&#1072;
                          &#1076;&#1083;&#1103; &#1088;&#1086;&#1079;&#1087;&#1086;&#1076;&#1110;&#1083;&#1077;&#1085;&#1086;&#1075;&#1086; &#1074;&#1080;&#1082;&#1086;&#1085;&#1072;&#1085;&#1085;&#1103; &#1085;&#1072;&#1082;&#1083;&#1072;&#1076;&#1072;&#1108; &#1076;&#1077;&#1103;&#1082;&#1110; &#1086;&#1073;&#1084;&#1077;&#1078;&#1077;&#1085;&#1085;&#1103;
                          &#1090;&#1086;&#1075;&#1086;, &#1097;&#1086; &#1084;&#1086;&#1078;&#1083;&#1080;&#1074;&#1077;. &#1053;&#1072;&#1081;&#1073;&#1110;&#1083;&#1100;&#1096; &#1086;&#1095;&#1077;&#1074;&#1080;&#1076;&#1085;&#1080;&#1084; &#1108; &#1090;&#1077;, &#1097;&#1086; &#1074;&#1089;&#1110;
