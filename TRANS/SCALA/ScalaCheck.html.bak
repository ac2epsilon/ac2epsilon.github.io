  (<span  class="pl-s"  style="box-sizing: border-box; color: rgb(24, 54, 145);"><span
 class="pl-pds"  style="box-sizing: border-box; color: rgb(24, 54, 145);">"</span>result > #1<span
 class="pl-pds"  style="box-sizing: border-box; color: rgb(24, 54, 145);">"</span></span>    <span
 class="pl-k"  style="box-sizing: border-box; color: rgb(167, 29, 93);">|:</span> res <span
 class="pl-k"  style="box-sizing: border-box; color: rgb(167, 29, 93);">>=</span> m) <span
 class="pl-k"  style="box-sizing: border-box; color: rgb(167, 29, 93);">&&</span>
  (<span  class="pl-s"  style="box-sizing: border-box; color: rgb(24, 54, 145);"><span
 class="pl-pds"  style="box-sizing: border-box; color: rgb(24, 54, 145);">"</span>result > #2<span
