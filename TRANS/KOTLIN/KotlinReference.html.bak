    <span class="cm-variable" style="color: rgb(0, 0, 0);">print</span>(<span class="cm-variable"
style="color: rgb(0, 0, 0);">enumValues</span><span class="cm-operator" style="color: rgb(0, 0, 0);">&lt;</span><span
class="cm-variable" style="color: rgb(0, 0, 0);">T</span><span class="cm-operator"
style="color: rgb(0, 0, 0);">&gt;</span>().<span class="cm-variable" style="color: rgb(0, 0, 0);">joinToString</span> { <span
class="cm-variable" style="color: rgb(0, 0, 0);">it</span>.<span class="cm-variable"
style="color: rgb(0, 0, 0);">name</span> })
}

<span class="cm-variable" style="color: rgb(0, 0, 0);">printAllValues</span><span
