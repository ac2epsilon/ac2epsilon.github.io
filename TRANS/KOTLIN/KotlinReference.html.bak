            <span class="cm-variable" style="color: rgb(0, 0, 0);">_table</span> <span
class="cm-operator" style="color: rgb(0, 0, 0);">=</span> <span class="cm-variable"
style="color: rgb(0, 0, 0);">HashMap</span>() <span class="cm-comment" style="color: gray;">// Типи параметрів виводяться</span>
        }
        <span class="cm-keyword" style="color: rgb(0, 55, 179); font-weight: 700;">return</span> <span
